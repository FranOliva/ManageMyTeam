<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <LinearLayout
        android:id="@+id/administrator__container__name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        android:orientation="vertical">

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/administration__create__coach__img"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:clickable="true"
            android:focusable="true"
            android:layout_marginTop="@dimen/text_thirty"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_gravity="end"
            app:srcCompat="@drawable/ic_right_arrow" />

        <TextView
            android:id="@+id/administration__label__create_coach_title"
            style="@style/Regular.Twenty.Black"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/administration_create_coach" />

    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>